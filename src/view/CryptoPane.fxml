<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CryptoPaneController">
   <children>
      <Label text="Key:" />
      <TextField fx:id="keyField" promptText="Key" />
      <Label fx:id="entryLbl" text="Label" textAlignment="CENTER" />
      <HBox spacing="5.0">
         <children>
            <TextField fx:id="entryField" />
            <Button fx:id="submitBtn" mnemonicParsing="false" text="Submit" />
         </children>
      </HBox>
      <Label text="Result:" />
      <TextField fx:id="resultField" editable="false" promptText="RESULT" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
